{
  "name": "data-pipeline",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-pipeline/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:util"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/data-pipeline/jest.config.ts"
      }
    },
    "cli": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts",
        "cwd": "{workspaceRoot}"
      }
    },
    "scrape": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts",
        "cwd": "{workspaceRoot}"
      }
    },
    "convert": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts convert",
        "cwd": "{workspaceRoot}"
      }
    },
    "enrich": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts enrich",
        "cwd": "{workspaceRoot}"
      }
    },
    "validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts validate",
        "cwd": "{workspaceRoot}"
      }
    },
    "import": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts import",
        "cwd": "{workspaceRoot}"
      }
    },
    "search": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx libs/data-pipeline/src/cli.ts search",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
